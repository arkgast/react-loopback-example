{
  "name": "react-loopback-example",
  "version": "1.0.0",
  "description": "Frontend of react-loopback-example",
  "main": "index.js",
  "scripts": {
    "build": "webpack --colors",
    "start": "env-cmd .env.development webpack-dev-server --colors",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": false,
    "verbose": true,
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/public"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/test/__mocks__/fileTransformer.js",
      "^Components(.*)$": "<rootDir>/src/components",
      "^Data(.*)$": "<rootDir>/src/data",
      "^Hocs(.*)$": "<rootDir>/src/hocs",
      "^Scenes(.*)$": "<rootDir>/src/scenes",
      "^Services(.*)$": "<rootDir>/src/services",
      "^Styles(.*)$": "<rootDir>/src/styles"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "repository": "",
  "author": "Arnold Gandarillas Castillo <arkgast@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.30",
    "material-ui-icons": "^1.0.0-beta.17",
    "proptypes": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.8",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.18",
    "compression-webpack-plugin": "^1.1.3",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "env-cmd": "^7.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.1.4",
    "offline-plugin": "^4.9.0",
    "react-hot-loader": "^4.0.0-beta.18",
    "react-imported-component": "^1.0.4",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-config": "^7.5.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.11.1"
  }
}
